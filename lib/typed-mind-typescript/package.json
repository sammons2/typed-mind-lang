{
  "name": "@sammons/typed-mind-typescript",
  "version": "0.1.5",
  "description": "TypeScript analysis and bridge to TypedMind DSL - extract architecture from TypeScript codebases",
  "keywords": [
    "typescript",
    "typedmind",
    "ast",
    "analyzer",
    "architecture",
    "extraction",
    "tsc"
  ],
  "author": "sammons",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sammons2/typed-mind-lang"
  },
  "homepage": "https://github.com/sammons2/typed-mind-lang#readme",
  "bugs": {
    "url": "https://github.com/sammons2/typed-mind-lang/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "typed-mind-ts": "./dist/cli.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "build": "tsup src/cli.ts src/index.ts --format cjs --dts --clean && chmod +x dist/cli.js",
    "dev": "tsup src/cli.ts src/index.ts --format cjs --dts --watch",
    "clean": "rm -rf dist *.tsbuildinfo",
    "test": "vitest --run",
    "test:watch": "vitest --watch",
    "lint": "eslint src",
    "prepublishOnly": "npm run clean && npm run build && npm test"
  },
  "dependencies": {
    "@sammons/typed-mind": "workspace:*",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@types/node": "^22.18.1",
    "tsup": "^8.3.5",
    "typescript-eslint": "^8.42.0",
    "vitest": "^3.2.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
